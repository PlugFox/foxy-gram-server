{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "[Go] Launch server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": []
        },
        {
            "name": "[Go] Launch current file",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
            "envFile": "${workspaceFolder}/.env",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": []
        }
    ]
}